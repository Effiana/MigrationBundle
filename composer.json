{
    "name": "effiana/migration-bundle",
    "type": "symfony-bundle",
    "description": "Schema and data migration for BAP",
    "keywords": ["BAP", "Migration", "Schema", "Data", "Fixtures"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Labudzinski",
            "email": "dominik@labudzinski.com",
            "homepage": "http://labudzinski.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 | ^7.4 | ^8.0",
        "doctrine/common": "^2.10",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/dbal": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "psr/container": "^1.0",
        "symfony/config": "^4.3 | ^5",
        "symfony/console": "^4.3 | ^5",
        "symfony/dependency-injection": "^4.3 | ^5",
        "symfony/event-dispatcher": "^4.3 | ^5",
        "symfony/finder": "^4.3 | ^5",
        "symfony/framework-bundle": "^4.2 | ^5",
        "symfony/http-kernel": "^4.3 | ^5",
        "twig/twig": "^3.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": { "Effiana\\MigrationBundle\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "apcu-autoloader": true
    }
}
